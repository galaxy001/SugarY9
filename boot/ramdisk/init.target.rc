on charger
    wait /dev/block/bootdevice/by-name/system
    mount ext4 /dev/block/bootdevice/by-name/system /system ro barrier=1

    #TINNO LiZidong: save off charge mode kernel log
    wait /dev/block/bootdevice/by-name/userdata
    mount ext4 /dev/block/bootdevice/by-name/userdata /data rw barrier=1

    #mount adb funfs
    mkdir /dev/usb-ffs 0770 shell shell
    mkdir /dev/usb-ffs/adb 0770 shell shell
    mount functionfs adb /dev/usb-ffs/adb uid=2000,gid=2000
    write /sys/class/android_usb/android0/f_ffs/aliases adb

    load_all_props
    setprop sys.usb.configfs 0
    setprop persist.sys.usb.config diag,adb
    start qcom-post-boot
    setprop persist.sys.usb.config diag,adb
    start hvdcp_opti
    start scc
    
#Tinno:CJ
on charger_calibration    

		setprop persist.radio.adb_log_on 1
		load_system_props
		
    wait /dev/block/bootdevice/by-name/modem
    mount vfat /dev/block/bootdevice/by-name/modem /firmware ro context=u:object_r:firmware_file:s0,shortname=lower,uid=1000,gid=1000,dmask=227,fmask=337
		rm /dev/.booting

    start qcom-usb-sh
    start logd
    start debuggerd
    start debuggerd64
    start srvmag_ffbm
    start per_mgr
    start ril-daemon
    start rmt_storage

    setprop persist.sys.ssr.restart_level ALL_ENABLE
    setprop persist.sys.ssr.enable_ramdumps 0
    

